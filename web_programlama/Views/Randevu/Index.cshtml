@model IEnumerable<Randevu>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.RandevuZamani)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IslemId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CalisanId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.KullaniciId)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @item.RandevuZamani
            </td>
            <td>
                @item.IslemId
            </td>
            <td>
                @item.CalisanId
            </td>
            <td>
                @item.KullaniciId
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
